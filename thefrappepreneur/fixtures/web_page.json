  "main_section_html": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Hub of Frappe Enthusiasts</title>\n    <style>\n        /* Basic Reset */\n        body {\n            margin: 0;\n            padding: 0;\n            font-family: Arial, sans-serif;\n            min-height: 100vh; /* Ensures the body takes the full height of the viewport */\n            display: flex;\n            flex-direction: column;\n            \n        }\n        button.btn-primary {     background-color: #000000 !important;     border-color: #000000 !important; }\n        #about {\n            padding: 20px;\n            background-color: #ffffff;\n            flex-grow: 1; /* Allows the about section to expand and fill available space */\n        }\n\n        .about-content {\n            display: flex;\n            align-items: center;\n            margin-bottom: 20px;\n        }\n\n        .video-container {\n            flex: 1;\n            width: 25%;\n            height: 300px; /* Set the desired height */\n            position: relative;\n            overflow: hidden;\n        }\n\n        .video-container video {\n            width: 100%;\n            height: 100%;\n            object-fit: cover; /* Ensures the video covers the container */\n        }\n\n        .text-content {\n            flex: 3;\n            width: 75%;\n            padding-left: 20px;\n        }\n\n        .text-content h2 {\n            margin-top: 0;\n        }\n\n        .text-content .highlight {\n            background-color: #ffff00; /* Highlight color */\n            font-weight: bold;\n        }\n\n        /* Fade-in and scale animation for stats items */\n        @keyframes fadeInScale {\n            0% {\n                opacity: 0;\n                transform: scale(0.9); /* Slightly smaller */\n            }\n            100% {\n                opacity: 1;\n                transform: scale(1); /* Normal size */\n            }\n        }\n\n        .stats-container {\n            margin-top: 75px; /* Add space before stats section */\n            display: flex;\n            justify-content: space-around;\n            padding: 10px 0;\n            background-color: #ffffff;\n            border: 1px solid #ddd;\n            border-radius: 5px;\n        }\n\n        .stats-item {\n            text-align: center;\n            flex: 1;\n            opacity: 0; /* Initially hidden */\n            animation: fadeInScale 1s ease forwards; /* Play the animation */\n        }\n\n        .stats-item:nth-child(1) {\n            animation-delay: 0.2s; /* Delayed effect for the first item */\n        }\n\n        .stats-item:nth-child(2) {\n            animation-delay: 0.4s; /* Delayed effect for the second item */\n        }\n\n        .stats-item:nth-child(3) {\n            animation-delay: 0.6s; /* Delayed effect for the third item */\n        }\n\n        .stats-item h3 {\n            margin: 0;\n            font-size: 2em;\n            color: #333;\n        }\n\n        .stats-item p {\n            margin: 0;\n            font-size: 1.2em;\n            color: #555;\n        }\n\n        /* Button Style */\n        .register-button {\n            display: inline-block;\n            padding: 10px 20px;\n            font-size: 1em;\n            color: #000000;\n            background-color: white;\n            border: none;\n            border-radius: 5px;\n            text-align: center;\n            cursor: pointer;\n            text-decoration: none;\n            margin-top: 20px;\n        }\n\n        .register-button:hover {\n            background-color: #0056b3;\n        }\n    </style>\n</head>\n<body>\n    <section id=\"about\" class=\"active\">\n        <div class=\"about-content\">\n            <div class=\"video-container\">\n                <video src=\"/files/home.mp4\" autoplay loop muted></video>\n            </div>\n            <div class=\"text-content\">\n                <h1>Hub of Frappe Enthusiasts</h1>\n                <p>Learn, Contribute, Network & Grow with Frappe Community</p>\n                <ul>\n                  <li>List & Find Profiles</li>\n                  <li>Read & Write Reviews</li>\n                  <li>Earn the Trust via Profile Rating</li>\n                </ul>\n                <p>What else Inside?</p>\n                <ul>\n                  <li>Vin Hire - Identify good companies and candidates</li>\n                  <li>Vin Lancer - Grab awesome oppourtunities</li>\n                  <li>Vin Decode - Bootcamps, Contests, Personalized Trainings</li>\n                </ul>\n                <a href=\"https://thefrappepreneur.in/registration/new\" class=\"btn btn-primary\" style=\"background-color: #000000; border-color: #000000; padding: 10px 20px; color: white; text-decoration: none;\">\n    Register Now\n</a>\n\n            </div>\n        </div>\n        \n        <div class=\"stats-container\">\n            <div class=\"stats-item\">\n                <h3>15+</h3>\n                <p>Members</p>\n            </div>\n            <div class=\"stats-item\">\n                <h3>10+</h3>\n                <p>Testimonials</p>\n            </div>\n            <div class=\"stats-item\">\n                <h3>8</h3>\n                <p>Hiring Partners</p>\n            </div>\n        </div>\n    </section>\n\n    <script>\n        document.addEventListener(\"DOMContentLoaded\", function() {\n            var registerButton = document.getElementById(\"register-button\");\n            var protocol = window.location.protocol;\n            var siteUrl = window.location.host; // Get the host (e.g., example.com)\n            var referralLink = `${protocol}//${siteUrl}/registration/new`; // Construct URL for new registration form\n\n            registerButton.href = referralLink;\n        });\n    </script>\n</body>\n</html>\n",
